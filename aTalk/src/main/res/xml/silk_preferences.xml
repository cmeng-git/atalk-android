<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto">
    <org.atalk.android.gui.settings.widget.ConfigCheckBox
        android:key="neomedia.codec.audio.silk.encoder.USE_FEC"
        android:title="@string/service_gui_settings_SILK_FEC" />

    <org.atalk.android.gui.settings.widget.ConfigCheckBox
        android:key="neomedia.codec.audio.silk.encoder.AWLAYS_ASSUME_PACKET_LOSS"
        android:title="@string/service_gui_settings_SILK_ASSUME_PL" />

    <org.atalk.android.gui.settings.widget.ConfigEditText
        android:inputType="numberDecimal"
        android:key="neomedia.codec.audio.silk.encoder.SPEECH_ACTIVITY_THRESHOLD"
        android:title="@string/service_gui_settings_SILK_FEC_SAT"
        custom:floatMax="1"
        custom:floatMin="0" />

    <org.atalk.android.gui.settings.widget.ConfigCheckBox
        android:key="neomedia.codec.audio.silk.ADVERTISE_FEC"
        android:title="@string/service_gui_settings_SILK_ADVERTISE_FEC" />
</PreferenceScreen>